    "eslint-plugin-react": "^7.21.5",
    "jest": "26.6.3",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-resolve": "24.7.1",
    "jest-sonar-reporter": "^2.0.0",
    "jest-watch-typeahead": "^1.0.0",
    "npm-run-all": "^4.1.5",
