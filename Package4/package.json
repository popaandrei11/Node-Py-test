  "scripts": {
    "start:local": "set HTTPS=true && craco start",
    "start": "craco start",
    "build": "craco  build && npm run build:version && cp ./public/index.html ./build",
    "build:version": "node ./craco/generateVersion.js > ./build/version.json",
    "test": "node scripts/test.js",
    "eject": "craco eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^axios$": "axios/dist/axios.js"
    },
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.jsx",
      "src/**/*.js",
      "src/**/*.tsx",
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/libs/**/*.js"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(css|scss)$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "globals": {}
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@playwright/test": "^1.38.1",
    "@types/jest": "^26.0.4",
    "@types/node": "^20.8.4",
    "@types/react-router-dom": "^5.3.3",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
